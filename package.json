{
  "name": "@851-labs/lazydev",
  "version": "0.1.2",
  "description": "lazygit-style log viewer for any command",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "lazydev": "./src/cli.tsx"
  },
  "scripts": {
    "dev": "bun run src/cli.tsx",
    "build": "bun build src/cli.tsx --outdir dist --target bun"
  },
  "dependencies": {
    "@types/yargs": "^17.0.35",
    "ink": "^5.2.0",
    "react": "^18.3.1",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
